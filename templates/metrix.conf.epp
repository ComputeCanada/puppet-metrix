Listen 9000

<VirtualHost *:9000>
  DocumentRoot "/var/www/metrix"

  ProxyPass /static/ !
  ProxyPass / http://127.0.0.1:8001/
  Alias /static "/var/www/metrix-static"

  <Directory "/var/www/metrix-static">
    Order allow,deny
    Allow from all
    Options Indexes FollowSymLinks MultiViews
    Satisfy Any
  </Directory>

</VirtualHost>
